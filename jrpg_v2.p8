function start_turn()
	battle_log={}
	p_actions = {}
	🐱_actions = {}
	drawn_line = 1
	drawn_char = {0,0,0}
end

function append_p(s)
	add(p_actions,s)
end

function append_🐱(s)
	add(🐱_actions,s)
end

function manage_turn()
	local first = p_actions
	local second = 🐱_actions
	if player.agi < 🐱[5] then
		first = 🐱_actions
		second = p_actions
	end
	foreach(first,append_battle_log)
	foreach(second,append_battle_log)
	p_actions = {}
	🐱_actions = {}
	turn_starting = true
end

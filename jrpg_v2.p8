--update and draw functions

function adventure_update()
	_drw = draw_game
	local found_npc = false
	player_ox = 0
	player_oy = 0
	if(f > 10) then
		player_frame += 1
		player_frame %= 2
		f = 0
	end
	if btnp(❎) or buffer❎ then
		buffer❎ = false
		sfx(4)
		_upd = menu_update
		_drw = draw_menu
	else
		if btn(⬆️) then
			found_npc = lookup_npc(player_x,player_y-1)
			if (not collide_check(player_x,player_y,"⬆️",1) and not found_npc) then
				player_y -= 1
				player_oy=8
				_upd = walking_update
			elseif found_npc then
				talk(player_x,player_y-1)
			else
				sfx(0)
			end
		elseif btn(⬇️) then
			found_npc = lookup_npc(player_x,player_y+1)
			if (not collide_check(player_x,player_y,"⬇️",1) and not found_npc) then
				player_y += 1
				player_oy=-8
				_upd = walking_update
			elseif found_npc then
				talk(player_x,player_y+1)
			else
				sfx(0)
			end
		end
		if btn(➡️) then
			found_npc = lookup_npc(player_x+1,player_y) 
			if (not collide_check(player_x,player_y,"➡️",1) and not found_npc) then
				player_x += 1
				player_ox=-8
				_upd = walking_update
			elseif found_npc then
				talk(player_x+1,player_y)
			else
				sfx(0)
			end
				side = false
		elseif btn(⬅️) then
			found_npc = lookup_npc(player_x-1,player_y)
			if (not collide_check(player_x,player_y,"⬅️",1) and not found_npc) then
				player_x-= 1
				player_ox=8
				_upd = walking_update
			elseif found_npc then
				talk(player_x-1,player_y)
			else
				sfx(0)
			end
				side = true
		end
	end
end

function draw_game()
	cls()
	palt(0,true)
	map(0,0,0,0,128,32)
	spr(player_spr+player_frame,player_x*8+player_ox,player_y*8+player_oy,1,1,side)
	draw_npcs()
end

function walking_update()
	if abs(player_ox)>0 then
		player_ox -= 1*sgn(player_ox)
	end
	if abs(player_oy)>0 then
		player_oy -= 1*sgn(player_oy)
	end	
	if player_ox == 0 and player_oy == 0 then
		_upd = adventure_update
		if enc_zone then
			enc_counter += 1
			if enc_counter > enc_steps then
				enc_counter = 0
				enc_steps = flr(rnd(enc_rate)) + 1
				🐱 = monsters[1]
				🐱♥ = 🐱[4]
				🐱s = {unpack(🐱)}
				😐s = {unpack(player)}
				battle_log = {}
				battle_commands = {}
				append_battle_log(🐱[1].." challenges you")
				drawn_line = 1
				drawn_char = {0,0,0}
				_upd = battle_update
				chars_printed = 0
				_drw = draw_battle
			end
		end
	end
	if btnp(❎) then
		buffer❎ = true
	end
end


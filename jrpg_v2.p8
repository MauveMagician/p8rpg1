--map collision

function collide_check(x,y,aim,flag)
	if aim=="⬅️" then
		return fget(mget(x-1,y), flag)
 elseif aim=="➡️" then
		return fget(mget(x+1,y), flag)
 elseif aim=="⬆️" then
		return fget(mget(x,y-1), flag) 	
 elseif aim=="⬇️" then
		return fget(mget(x,y+1), flag) 	
 end
end

function lookup_npc(x,y)
	for v in all(npc_list) do
		if x == v[1] then
			if (y == v[2]) return true
		end
	end
	return false
end

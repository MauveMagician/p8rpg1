--player actions in combat
function escape()
	append_p("you try to run...")
	if not 🐱[10] and (flr(rnd(player.agi)) > flr(rnd(🐱[5]))) then
		append_p("and get away safely!")
		sfx(2)
		append_p("")
		battle_commands[""] = function() _upd = adventure_update end
		_upd = battle_update
		return true
	end
	append_p("you can't escape the "..🐱[1])
	_upd = battle_update
	return true
end

function player_attack()
	local text_call = "you attack the "..🐱[1]
	append_p(text_call)
 dmg = damage_calc()
	text_call = "you deal "..tostr(dmg).." damage"
	append_p(text_call)
	battle_commands[text_call] = player_hit
	_upd = battle_update
	return true
end

function player_cure()
		local text_call = ""
		if sub(inventory[fight_cursor],1,1) == "●" then
			text_call = "you try casting "..sub(inventory[fight_cursor],2)
		else
			text_call = "you try healing yourself"
		end
		append_p(text_call)
		for i in all(items) do
			if i[2] == inventory[fight_cursor] then
					if player.● > i[5] then
						player.● -= i[5]
						if i[6] < 1 then
							text_call = "you restore "..flr(player.m♥*i[6]).."♥"
							battle_commands[text_call] = function() gain_health(flr(player.m♥*i[6])) end
						elseif i[6] != 1 then
							text_call = "you restore "..i[6].."♥"
							battle_commands[text_call] = function() gain_health(i[6]) end
						else
							text_call = "you restore all your ♥"
							battle_commands[text_call] = function() gain_health(player.m♥) end
						end
					else
						text_call = "you don't have enough ●"
					end
					append_p(text_call)
					break
				end
			end
	_upd = battle_update
	return true
	end

function gain_health(amount)
	player.♥ += amount
	if(player.♥>player.m♥) player.♥=player.m♥
	screen_effect(11,6,_modulate)
end

function player_hit(i)
	🐱♥-= dmg
	screen_effect(0,12,_flash)
	check_loss(i)
	check_win(i)
end

function damage_calc()
	local lookup = inventory[fight_cursor]
	local damage = 0
	for i in all(items) do
		if i[2] == lookup then
			if i[1]==1 then
				damage = player.str * i[5]
			end
		end
	end
	return damage
end

function check_win(i)
		if 🐱♥ < 1 then
			battle_log = {unpack(battle_log,1,i)}
			battle_commands = {}
			local xp_gain = max(0,🐱[7] - player.lv)
			append_battle_log("the "..🐱[1].." is defeated")
			if (xp_gain > 0) append_battle_log("you get "..xp_gain.."★")
			append_battle_log("you earn "..🐱[8].."g")
			player.xp += xp_gain
			while player.xp > 9 do
				player.xp -= 10
				player.lv += 1
				local ♥boost = flr(rnd(5)+rnd(5)+2)
				local ●boost = flr(rnd(5)+rnd(5)+2)
				local strboost = flr(rnd(2)+rnd(2)+2)
				local agiboost = flr(rnd(2)+rnd(2)+2)
				append_battle_log("you have achieved lv "..player.lv)
				append_battle_log("max ♥ up by "..tostr(♥boost))
				append_battle_log("max ● up by "..tostr(●boost))
				append_battle_log("strength up by "..tostr(strboost))
				append_battle_log("agility up by "..tostr(agiboost))
				player.m♥ += ♥boost
				player.m● += ●boost
				player.str += strboost
				player.agi += agiboost
			end
			g += 🐱[8]
			append_battle_log("  ")
			battle_commands["  "] = function() _upd = adventure_update end
		end
end

function check_loss(i)
	if player.♥ < 1 then
			battle_log = {unpack(battle_log,1,i)}
			battle_commands = {}
			append_battle_log("you are defeated")
			append_battle_log("you lose all ★ and g")
			append_battle_log("but you will live")
			append_battle_log("")
			player.xp = 0
			g = 0
			player.♥ = 0
			player_x = 8
			player_y = 7
			battle_commands[""] = function() player.♥ = 1 player.● = 0 _upd = adventure_update end
	end
end


function screen_effect(c,d,e)
	pal(0,c,1)
	countf = 0
	countd = d
	if _upd != _modulate and _upd != _shake and _upd != _flash then
		p_upd = _upd
		p_drw = _drw
	end
	pixels = {}
	for i=0,128,1 do
		add(pixels,{})
		for j=0,128,1 do
			add(pixels[i+1],pget(i,j))
		end
	end
	_upd = e
	_drw = draw_inact
end

function _shake()
	shake = true
	_modulate()
end

function _modulate()
	countf += 1
	if (shake and countf > countd/2) pal(0,0,1)
	if countf > countd then
		pal(0,0,1)
		shake=false
		flash=false
		_upd = p_upd
		_drw = p_drw
		for i=0,128,1 do
			for j=0,128,1 do
				pset(i,j,pixels[i+1][j+1])
			end
		end
	end
end

function _flash()
	flash=true
	_modulate()
end

function draw_inact()
	if flash then
		if countf%4 == 0 then
			rectfill(32,16,96,72,0)
			rect(32,16,96,72,7)
		else
			for i=0,128,1 do
				for j=0,128,1 do
					pset(i,j,pixels[i+1][j+1])
				end
			end
		end
	end
	if shake then
		cls()
		coffset_x = rnd(12) - 6
		coffset_y = rnd(12) - 6
		for i=0,128,1 do
			for j=0,128,1 do
				pset(i+coffset_x,j+coffset_y,pixels[i+1][j+1])
			end
		end
	end
end

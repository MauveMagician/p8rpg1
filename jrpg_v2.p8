drawn_line = 1
drawn_char = {0,0,0}
done_drawing = false
function battle_update()
	_drw = draw_battle
	if drawn_line < 4 then
		drawn_char[drawn_line] += 1
		for i=0,drawn_line,1 do
			if battle_commands[battle_log[i]] then
				battle_commands[battle_log[i]](i)
				battle_commands[battle_log[i]] = nil
			end
		end
	end
	if drawn_char[min(count(battle_log),3)] == #battle_log[min(count(battle_log),3)] then
		done_drawing = true
	end
	if btnp(🅾️) and done_drawing then
		if update_battle_log() then
			start_turn()
			_upd = command_update
			done_drawing = false
		else
			drawn_line = 3
			drawn_char[3] = 0
			done_drawing = false
		end
	end	
end

function draw_battle()
	palt(0,false)
	rectfill(32,16,96,72,0)
	rect(32,16,96,72,7)
	sx, sy = (🐱[2] % 16) * 8, (🐱[2] \ 16) * 8
	sspr(sx,sy,16,16,48,32,32,32)
	rectfill(4,74,124,108,0)
	rect(4,74,124,108,7)
	print_battle_log()
	if done_drawing then
		local buttony = 100
		if abs(f)%12 == 0 then
			buttony = 101
		end
		print("🅾️",112,buttony)
	end
	draw_char_sheet()
end

function print_battle_log()
	for i=1,drawn_line-1,1 do
		print(battle_log[i],8,73+8*i)
	end
	if battle_log[drawn_line] and drawn_line < 4 then
		local print_string = sub(battle_log[drawn_line],0,drawn_char[drawn_line])
		print(print_string,8,73+8*drawn_line)
		if #print_string >= #battle_log[drawn_line] then
			drawn_line += 1
		end
	end
end

function update_battle_log()
	if count(battle_log) < 4 then 
		return true
	else
		local looped=false
		for i=2,count(battle_log),1 do
			battle_log[i-1] = battle_log[i]
			looped=true
		end
		if looped then
			deli(battle_log,#battle_log)
		end
	end
end

function append_battle_log(s)
		add(battle_log,s)
end

function fill_battle_log()
	while count(battle_log) < 3 do
		append_battle_log(" ")
	end
end

